{"head":"<title>resonate</title>\n  <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css\">\n  <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootswatch/3.3.5/paper/bootstrap.min.css\">\n  <script src=\"api.js\"></script>\n  <script src=\"https://code.jquery.com/jquery-2.1.4.min.js\"></script>\n  <script src=\"https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.6/d3.min.js\"></script>\n  <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js\"></script>","body":"","js":"\nTemplate.body.addContent((function() {\n  var view = this;\n  return [ HTML.Raw('<div id=\"branding\" class=\"col-lg-offset-2 col-lg-8 text-center\">\\n    <h1>resonate</h1>\\n    <hr>\\n  </div>\\n  <div id=\"chartDescription\" class=\"col-lg-12\"><h3>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Cum in illo modi vitae eaque perspiciatis et labore est voluptate voluptatibus quibusdam suscipit, asperiores nihil. Officia ex rem ratione adipisci quasi.</h3></div>\\n  '), HTML.DIV({\n    id: \"chart1\",\n    \"class\": \"col-lg-12 text-center\"\n  }, \"\\n    \", HTML.SVG({\n    id: \"povertyChart\"\n  }), \"\\n  \"), HTML.Raw('\\n  <div id=\"chartUnderText\" class=\"col-lg-12 text-center\">\\n  <h1>Info on the Json data</h1>\\n  </div>\\n  <div id=\"buttonsToChangeChart\" class=\"col-lg-4\"> \\n  <button class=\"btn btn-primary\" value=\"Male\">Male</button>\\n  <button class=\"btn\" value=\"Female\">Female</button>\\n  </div>\\n  '), HTML.DIV({\n    id: \"resultOfButtonPress\",\n    \"class\": \"col-lg-12\"\n  }, \"\\n      \", Spacebars.include(view.lookupTemplate(\"percent_affected_chart\")), \"\\n  \"), \"\\n\\n  \", HTML.SCRIPT('\\n  d3.json(\"client/data.json\", function(err, info) {\\n  var svg = d3.select(\"#povertyChart\");\\n\\n  var data = info.povertyRates;\\n  console.log(data);\\n\\n  var cHeight = 500;\\n\\n  var maxRate = d3.max(data, function(d) { return d.rate } );\\n\\n  var yScale = d3.scale.linear()\\n    .domain([11, maxRate])\\n    .range([cHeight, 0])\\n\\n  var g = svg.append(\"g\").attr(\"transform\", \"translate(95, 50)\");\\n\\n  var circles = g.selectAll(\"circle\").data(data);\\n\\n  circles.enter()\\n  .append(\"circle\")\\n  .attr({\\n    cx:function(d,i){return 49 + i * 22},\\n    cy:function(d,i){return yScale(d.rate)},\\n    r:9\\n  })\\n  .on(\\'mouseover\\', function(d) {\\n    d3.select(this).append(\"svg:title\").text(\"yesy\")\\n    console.log(\"yup\")\\n  });\\n});\\n') ];\n}));\nMeteor.startup(Template.body.renderToDocument);\n\nTemplate.__checkName(\"percent_affected_chart\");\nTemplate[\"percent_affected_chart\"] = new Template(\"Template.percent_affected_chart\", (function() {\n  var view = this;\n  return [ HTML.P(Blaze.View(\"lookup:percent\", function() {\n    return Spacebars.mustache(view.lookup(\"percent\"));\n  }), \" of \", Blaze.View(\"lookup:category\", function() {\n    return Spacebars.mustache(view.lookup(\"category\"));\n  }), \" will be affected by this\"), \"\\n  \", HTML.SVG({\n    id: \"barChart\"\n  }) ];\n}));\n","bodyAttrs":{"class":"col-lg-12"}}